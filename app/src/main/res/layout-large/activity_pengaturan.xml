<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingLeft="5dp"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context="com.alpha.aqra.Pengaturan"
    android:background="@drawable/bg_pengaturan">

    <ImageButton
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:id="@+id/back_pengaturan"
        android:background="@drawable/btn_pengaturan"
        android:onClick="onButtonPengaturanClick"
        android:layout_alignParentBottom="true"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true" />

    <ImageView
        android:layout_width="50dp"
        android:layout_height="45dp"
        android:id="@+id/imageButton"
        android:background="@drawable/music_note"
        android:layout_marginLeft="32dp"
        android:layout_above="@+id/back_pengaturan"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_marginStart="32dp"
        android:layout_marginBottom="335dp" />

    <ImageView
        android:layout_width="45dp"
        android:layout_height="45dp"
        android:id="@+id/imageView5"
        android:background="@drawable/audio_volume"
        android:layout_alignTop="@+id/imageButton"
        android:layout_alignLeft="@+id/imageButton"
        android:layout_alignStart="@+id/imageButton"
        android:layout_marginTop="49dp"
        android:layout_marginLeft="5dp" />

    <SeekBar
        android:layout_width="380dp"
        android:layout_height="wrap_content"
        android:id="@+id/seekBar1"
        android:progressDrawable="@drawable/progress"
        android:visibility="visible"
        android:thumb="@drawable/thumb"
        android:layout_alignBottom="@+id/imageButton"
        android:layout_toRightOf="@+id/imageButton"
        android:layout_toEndOf="@+id/imageButton"
        android:layout_marginLeft="10dp"
        android:layout_marginBottom="10dp" />

    <SeekBar
        android:layout_width="380dp"
        android:layout_height="wrap_content"
        android:id="@+id/seekBar2"
        android:progressDrawable="@drawable/progress"
        android:visibility="visible"
        android:thumb="@drawable/thumb"
        android:layout_alignBottom="@+id/imageView5"
        android:layout_alignLeft="@+id/seekBar1"
        android:layout_alignStart="@+id/seekBar1"
        android:layout_marginBottom="13dp" />

    <ImageButton
        android:layout_width="75dp"
        android:layout_height="75dp"
        android:id="@+id/imageButton6"
        android:background="@drawable/btn_twitter"
        android:layout_marginTop="69dp"
        android:layout_below="@+id/imageView5"
        android:layout_alignLeft="@+id/seekBar2"
        android:layout_alignStart="@+id/seekBar2" />

    <ImageButton
        android:layout_width="245dp"
        android:layout_height="55dp"
        android:id="@+id/imageButton3"
        android:background="@drawable/btn_kebijakan_pribadi"
        android:layout_alignTop="@+id/imageButton5"
        android:layout_alignRight="@+id/seekBar2"
        android:layout_alignEnd="@+id/seekBar2"
        android:layout_marginRight="-25dp" />

    <ImageButton
        android:layout_width="75dp"
        android:layout_height="75dp"
        android:id="@+id/imageButton4"
        android:background="@drawable/btn_facebook"
        android:layout_alignTop="@+id/imageButton9"
        android:layout_toRightOf="@+id/imageButton9"
        android:layout_toEndOf="@+id/imageButton9"
        android:layout_marginLeft="5dp" />

    <ImageButton
        android:layout_width="75dp"
        android:layout_height="75dp"
        android:id="@+id/imageButton7"
        android:background="@drawable/btn_instagram"
        android:layout_alignTop="@+id/imageButton6"
        android:layout_toRightOf="@+id/imageButton6"
        android:layout_toEndOf="@+id/imageButton6"
        android:layout_marginLeft="5dp" />

    <ImageButton
        android:layout_width="245dp"
        android:layout_height="55dp"
        android:id="@+id/imageButton5"
        android:background="@drawable/btn_persyaratan_pelanggan"
        android:layout_below="@+id/imageButton6"
        android:layout_alignLeft="@+id/imageButton"
        android:layout_alignStart="@+id/imageButton"
        android:layout_marginTop="34dp"
        android:layout_marginLeft="-30dp" />

    <ImageButton
        android:layout_width="245dp"
        android:layout_height="55dp"
        android:id="@+id/imageButton8"
        android:background="@drawable/btn_pelayanan_pelanggan"
        android:layout_below="@+id/imageButton3"
        android:layout_toLeftOf="@+id/imageButton4"
        android:layout_toStartOf="@+id/imageButton4"
        android:layout_marginTop="5dp"
        android:layout_marginRight="-50dp" />

    <ImageButton
        android:layout_width="75dp"
        android:layout_height="75dp"
        android:id="@+id/imageButton9"
        android:background="@drawable/btn_gmail"
        android:layout_alignTop="@+id/imageButton7"
        android:layout_toRightOf="@+id/imageButton7"
        android:layout_toEndOf="@+id/imageButton7"
        android:layout_marginLeft="5dp" />

</RelativeLayout>
