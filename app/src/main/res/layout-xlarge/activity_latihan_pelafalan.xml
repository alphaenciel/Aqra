    <ImageButton
        android:layout_width="75dp"
        android:layout_height="55dp"
        android:id="@+id/latihan_dho"
        android:background="@drawable/hijayah_putih_dho"
        android:onClick="OnButtonPelafalanClick"
        android:layout_above="@+id/latihan_ma"
