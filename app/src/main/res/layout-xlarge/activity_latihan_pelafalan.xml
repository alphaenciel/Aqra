        android:layout_marginLeft="5dp" />

    <ImageButton
        android:layout_width="75dp"
        android:layout_height="55dp"
        android:id="@+id/latihan_ka"
        android:background="@drawable/hijayah_putih_ka"
        android:onClick="OnButtonPelafalanClick"
        android:layout_below="@+id/latihan_sa"
        android:layout_toLeftOf="@+id/latihan_qo"
        android:layout_toStartOf="@+id/latihan_qo" />

    <ImageButton
        android:layout_width="75dp"
        android:layout_height="55dp"
        android:id="@+id/latihan_la"
        android:background="@drawable/hijayah_putih_la"
