        android:background="@drawable/hijayah_putih_haq"
        android:onClick="OnButtonPelafalanClick"
        android:layout_alignTop="@+id/latihan_ya"
        android:layout_toLeftOf="@+id/latihan_wa"
        android:layout_toStartOf="@+id/latihan_wa" />

    <ImageButton
        android:layout_width="75dp"
        android:layout_height="55dp"
        android:id="@+id/latihan_ya"
        android:background="@drawable/hijayah_putih_ya"
        android:onClick="OnButtonPelafalanClick"
        android:layout_below="@+id/latihan_ain"
        android:layout_toLeftOf="@+id/latihan_haq"
        android:layout_toStartOf="@+id/latihan_haq" />

    <ImageButton
        android:layout_width="120dp"
        android:layout_height="120dp"
        android:id="@+id/latihan_btnRekam"
        android:background="@drawable/btn_rekam_off2"
        android:onClick="onClickLatihanRecord"
        android:layout_above="@+id/latihan_instruksi"
        android:layout_toLeftOf="@+id/latihan_tsa"
        android:layout_toStartOf="@+id/latihan_tsa" />

    <ImageView
        android:layout_width="300dp"
        android:layout_height="150dp"
        android:id="@+id/latihan_instruksi"
        android:background="@drawable/ket_rekam_audio2"
        android:layout_above="@+id/latihan_btnHome"
        android:layout_centerHorizontal="true"
        android:layout_marginBottom="5dp" />

    <ImageView
        android:layout_width="150dp"
        android:layout_height="140dp"
        android:id="@+id/latihan_status"
        android:layout_alignBottom="@+id/latihan_instruksi"
        android:layout_toRightOf="@+id/latihan_ka"
        android:layout_toEndOf="@+id/latihan_ka" />

    <ProgressBar
        style="?android:attr/progressBarStyleHorizontal"
        android:layout_width="150dp"
        android:layout_height="40dp"
        android:id="@+id/progressBar"
        android:progressDrawable="@drawable/blueprogress"
        android:layout_below="@+id/textView"
        android:layout_alignLeft="@+id/textView"
        android:layout_alignStart="@+id/textView"
        android:layout_marginLeft="5dp" />

    <ImageButton
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:id="@+id/latihan_tempSelected" />

    <TextView
        android:layout_width="150dp"
        android:layout_height="65dp"
        android:textAppearance="?android:attr/textAppearanceSmall"
        android:id="@+id/textView"
        android:background="@drawable/text_ketepatan_rate"
        android:layout_alignTop="@+id/latihan_instruksi"
        android:layout_toLeftOf="@+id/latihan_zo"
        android:layout_toStartOf="@+id/latihan_zo"
        android:layout_marginLeft="5dp"
        android:layout_marginRight="-25dp" />

